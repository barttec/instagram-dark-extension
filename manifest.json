{
    "name": "Instagram Dark Theme",
    "version": "1.1.0",
    "description": "Dark Theme for Instagram",
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "css": [
                "contentstyle.css"
            ],
            "matches": [
                "*://*.instagram.com/*"
            ]
        }
    ],
    "web_accessible_resources": [
        "style.css",
        "dark.png"
    ],
    "permissions": [
        "storage"
    ]
}